{
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "react",
        "lib": [
            "es5",
            "es2017",
            "dom",
            "esnext"
        ],
        "target": "es5",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
    },
    "include": ["src/**/*", "cypress/**/*"],
    "exclude": ["node_modules"]
}
