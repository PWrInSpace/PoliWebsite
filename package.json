{
    "name": "frontend",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "vite",
        "dev-host": "vite --host",
        "dev:debug": "vite --debug --force",
        "build": "cross-env NODE_ENV=development tsc --noEmit && vite build --mode development",
        "typecheck": "tsc --noEmit",
        "lintstylish": "eslint src/**/*.tsx --format stylish",
        "lint": "eslint src/**/*.tsx --format junit --o lint-results.xml",
        "lintFix": "eslint src/**/*.tsx --fix",
        "production": "cross-env NODE_ENV=production npm run lintstylish && tsc --noEmit && vite build --mode production",
        "ghPages": "cross-env NODE_ENV=ghPages npm run lintstylish && tsc --noEmit && vite build --mode production",
        "test": "jest",
        "e2e-dev": "cypress open",
        "e2e-local": "cypress run"
    },
    "license": "ISC",
    "devDependencies": {
        "@types/content-disposition": "^0.5.3",
        "@types/gtag.js": "0.0.10",
        "@types/jest": "^25.2.3",
        "@types/js-cookie": "^2.2.6",
        "@types/react": "^18.0.25",
        "@types/react-custom-scrollbars": "^4.0.6",
        "@types/react-dom": "^18.0.8",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "autoprefixer": "^9.8.0",
        "axios": "^0.19.2",
        "content-disposition": "^0.5.3",
        "cross-env": "^7.0.2",
        "cypress": "^11.0.1",
        "eslint": "^8.27.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "fs-extra": "^8.1.0",
        "jest": "^25.5.4",
        "js-cookie": "^3.0.1",
        "postcss": "^8.4.19",
        "postcss-scss": "^4.0.6",
        "query-string-es5": "^6.1.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.1.13",
        "sass": "^1.56.0",
        "schema-utils": "^2.7.0",
        "shorthash": "0.0.2",
        "typescript": "^4.8.4",
        "vite-plugin-checker": "^0.5.1",
        "vite-plugin-plain-text": "^1.2.1",
        "vite-plugin-progress": "^0.0.6"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.0.2",
        "@swc/core": "^1.3.14",
        "@tinymce/tinymce-react": "^4.2.0",
        "@types/react-helmet": "^6.1.6",
        "@vitejs/plugin-react": "^2.2.0",
        "babel-polyfill": "^6.26.0",
        "eslint-plugin-react": "^7.31.11",
        "i18next": "^22.4.9",
        "i18next-browser-languagedetector": "^7.0.1",
        "lodash": "^4.17.21",
        "math-interpolate": "^5.0.0",
        "memoize-one": "^5.1.1",
        "postcss-js": "^4.0.0",
        "prettier": "^2.7.1",
        "react-custom-scrollbars-2": "^4.4.0",
        "react-helmet": "^6.1.0",
        "react-router-dom": "^6.7.0",
        "terser": "^5.15.1",
        "unplugin-swc": "^1.3.2",
        "vite": "^3.2.3",
        "vite-plugin-sass-dts": "^1.2.13"
    }
}
